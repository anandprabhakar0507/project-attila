{
"manifest_version": 2,
    "name": "project-attila",
    "description": "Extension that evaluates math expressions",
    "version": "1.0",
    "browser_action": {
	"default_popup": "popup.html",
	"default_title": "project-attila"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "attila.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
	"activeTab",
	"https://ajax.googleapis.com/"
    ]
}
