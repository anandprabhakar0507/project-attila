{
"manifest_version": 2,
    "name": "project-attila",
    "short_name": "attila",
    "description": "Extension that evaluates math expressions",
    "author": "Noah Kim, Ian Fisher",
    "version": "0.0.0.1",
    "browser_action": {
	"default_popup": "popup.html",
	"default_title": "project-attila"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*",
		"file:///*/*"
            ],
	    "all_frames": true,
            "js": [
		            "jquery-3.2.1.js",
                "attila.js",
                "jquery-ui.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
	"activeTab",
	"https://ajax.googleapis.com/"
    ]
}
